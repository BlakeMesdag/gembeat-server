<%= form_for(vulnerability, html: { class: "form"}) do |f| %>

  <div class="control-group">
    <%= f.label :dependency_name, class: "control-label" %>
    <div class="controls">
      <%= f.text_field :dependency_name, class: "input-xxlarge" %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :identifier, class: "control-label"%>
    <div class="controls">
      <%= f.text_field :identifier, class: "input-xxlarge" %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :url, class: "control-label" %>
    <div class="controls">
      <%= f.text_field :url, class: "input-xxlarge" %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :description, class: "control-label" %>
    <div class="controls">
      <%= f.text_area :description, class: "input-xxlarge", rows: "10" %>
    </div>
  </div>

  <div class="control-group">
    <%= f.label :versions, class: "control-label" %>
    <div class="controls">
      <%= f.text_field :versions, class: "input-xxlarge", placeholder: "3.2.12, 3.1.11, 3.0.20" %>
    </div>
  </div>

  <div class="control-group">
    <div class="controls">
      <%= f.submit "Save", class: "btn btn-primary" %> <%= link_to "Cancel", url_for(vulnerability), class: "btn" %>
    </div>
  </div>
<% end %>